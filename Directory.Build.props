<Project>
    <PropertyGroup>
        <ParentDirectoryBuildPropsPath>$([MSBuild]::GetPathOfFileAbove('Default.Build.props', '$(MSBuildThisFileDirectory)..\'))</ParentDirectoryBuildPropsPath>
    </PropertyGroup>

    <ImportGroup>
        <Import Project="$(ParentDirectoryBuildPropsPath)" Condition="Exists($(ParentDirectoryBuildPropsPath))" />
    </ImportGroup>
    <ImportGroup>
        <Import Project="Default.Build.props" Condition="Exists('Default.Build.props') And !Exists($(ParentDirectoryBuildPropsPath))" />
    </ImportGroup>

    <PropertyGroup>
	<ImplicitUsings>false</ImplicitUsings>
	<Features>enable-generator-cache</Features>
	<LangVersion>preview</LangVersion>
	<Nullable>enable</Nullable>
	<RunAnalyzersDuringLiveAnalysis>false</RunAnalyzersDuringLiveAnalysis>
	<EnableNETAnalyzers>false</EnableNETAnalyzers>
	<DisableImplicitNamespaceImports>true</DisableImplicitNamespaceImports>	
	<GitVersionBaseDirectory>$(MSBuildThisFileDirectory)</GitVersionBaseDirectory>
	<Company>DragonSpark Technologies Incorporated</Company>
	<Copyright>Copyright Â© 2025 DragonSpark Technologies Incorporated</Copyright>
	<Trademark>DragonSpark Technologies Incorporated</Trademark>
	<AccelerateBuildsInVisualStudio>true</AccelerateBuildsInVisualStudio>
	<MetalamaAllowPreviewLanguageFeatures>true</MetalamaAllowPreviewLanguageFeatures>
  </PropertyGroup>
  <ItemGroup>
		<PackageReference Include="Microsoft.Net.Compilers.Razor.Toolset" Condition=" '$(UsingMicrosoftNETSdkRazor)' == 'true' " />
	</ItemGroup>
</Project>