<local:Setup x:Class="DragonSpark.Testing.Framework.DefaultSetup"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	xmlns:local="clr-namespace:DragonSpark.Testing.Framework"
	xmlns:autoMoq="clr-namespace:Ploeh.AutoFixture.AutoMoq;assembly=Ploeh.AutoFixture.AutoMoq"
	xmlns:setupPlatform="clr-namespace:DragonSpark.Windows.Setup;assembly=DragonSpark.Windows"
	xmlns:setup="clr-namespace:DragonSpark.Setup;assembly=DragonSpark"
	xmlns:dragonSpark="clr-namespace:DragonSpark;assembly=DragonSpark"
	mc:Ignorable="d" 
>
	<local:AssignServiceLocatorCommand />
	<setupPlatform:SetupRegistrationByConventionCommand />
	<setup:SetupUnityContainerCommand>
		<setup:SetupUnityContainerCommand.Instances>
			<setup:UnityInstance>
				<dragonSpark:ApplicationInformation DeploymentDate="2/1/2016" CompanyUri="http://framework.dragonspark.us/testing" />
			</setup:UnityInstance>
		</setup:SetupUnityContainerCommand.Instances>
	</setup:SetupUnityContainerCommand>
	<!--<setupPlatform:InitializeMarkupCommand />-->
	<local:CustomizeFixtureCommand>
		<local:ServiceLocationCustomization />
		<local:OutputCustomization />
		<local:LoggingRegistrationCustomization />
		<autoMoq:AutoConfiguredMoqCustomization />
	</local:CustomizeFixtureCommand>
</local:Setup>