@typeparam TItem

@inherits ManyContentTemplateComponentBase<ICollection<TItem>>

<ContentView TValue="ICollection<TItem>" Content="Content" NotFoundTemplate="@NotAssignedTemplate">
	@if (context.Count > 0)
	{
		@ChildContent(context)
	}
	else
	{
		@EmptyElementsTemplate
	}
</ContentView>

@code {
	[Parameter]
	public ICollection<TItem>? Content { get; set; }
}