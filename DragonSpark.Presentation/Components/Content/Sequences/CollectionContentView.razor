@typeparam TItem

@inherits Templates.ManyContentTemplateComponentBase<IReadOnlyCollection<TItem>>

<ContentView TValue="IReadOnlyCollection<TItem>" Content="Content" NotFoundTemplate="@NotFoundTemplate">
	@if (context.Count > 0)
	{
		@ChildContent(context)
	}
	else
	{
		@EmptyElementsTemplate
	}
</ContentView>

@code {
	[Parameter]
    public IReadOnlyCollection<TItem>? Content { get; set; }
}