@typeparam TItem

@inherits DragonSpark.Presentation.Components.Content.Templates.ManyContentTemplateComponentBase<ICollection<TItem>>

<ContentView TValue="ICollection<TItem>" Content="Content" NotFoundTemplate="@NotFoundTemplate">
	@if (context.Count > 0)
	{
		@ChildContent(context)
	}
	else
	{
		@EmptyElementsTemplate
	}
</ContentView>

@code {
	[Parameter]
	public ICollection<TItem>? Content { get; set; }
}