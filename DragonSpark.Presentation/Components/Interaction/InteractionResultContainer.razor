@using DragonSpark.Application.Model.Interaction
@inherits Components.ComponentBase

<CascadingValue TValue="IInteractionResult" Value="@Result">
	@ChildContent
</CascadingValue>

@code {
	[Parameter]
	public IInteractionResult Result { get; set; } = default!;

	[Parameter]
	public RenderFragment ChildContent { get; set; } = default!;
}