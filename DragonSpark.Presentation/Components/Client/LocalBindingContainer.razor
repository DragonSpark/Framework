@typeparam T

@using DragonSpark.Presentation.Environment.Browser
@inherits ComponentStateComponentBase

<LocalStateComponent T="T" Owner="Owner" Qualifier="@Qualifier">
    <ClientValueBindingComponent T="T" DefaultValue="@DefaultValue" Store="context" ChildContent="@ChildContent" />
</LocalStateComponent>

@code {
    [Parameter]
    public T DefaultValue { get; set; } = default!;

    [Parameter]
    public RenderFragment<Binding<T>> ChildContent { get; set; } = default!;
}