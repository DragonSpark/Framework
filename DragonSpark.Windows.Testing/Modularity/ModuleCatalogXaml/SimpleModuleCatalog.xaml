<modularity1:ModuleCatalog xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				xmlns:sys="clr-namespace:System;assembly=mscorlib"
			   xmlns:modularity="clr-namespace:DragonSpark.Windows.Modularity;assembly=DragonSpark.Windows"
			   xmlns:modularity1="clr-namespace:DragonSpark.Modularity;assembly=DragonSpark">
	<modularity:DynamicModuleInfoGroup Ref="ModuleGroup1" InitializationMode="OnDemand">
		<modularity1:ModuleInfo ModuleName="Module1" ModuleType="Module1Type" />
	</modularity:DynamicModuleInfoGroup>
	<modularity:DynamicModuleInfoGroup Ref="ModuleGroup2" InitializationMode="WhenAvailable">
		<modularity:DynamicModuleInfo ModuleName="Module2InModuleGroup2" ModuleType="Module2Type">
			<modularity1:ModuleInfo.DependsOn>
				<sys:String>ModuleW</sys:String>
			</modularity1:ModuleInfo.DependsOn>
		</modularity:DynamicModuleInfo>
		<modularity1:ModuleInfo ModuleName="Module3InModuleGroup2" ModuleType="ModuleType">
		</modularity1:ModuleInfo>
	</modularity:DynamicModuleInfoGroup>
	<modularity:DynamicModuleInfo Ref="Module3" ModuleName="Module3" ModuleType="Module3Type" />
</modularity1:ModuleCatalog>