<windows:Setup x:Class="DragonSpark.Testing.Client.Application.Setup"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:windows="clr-namespace:DragonSpark.Client.Windows;assembly=DragonSpark.Client.Windows"
	xmlns:application="clr-namespace:DragonSpark.Testing.Client.Application"
	xmlns:commands="clr-namespace:DragonSpark.Common.IoC.Commands;assembly=DragonSpark.Common"
	xmlns:activationCommands="clr-namespace:DragonSpark.Activation.IoC.Commands;assembly=DragonSpark"
	xmlns:forms="clr-namespace:DragonSpark.Client.Windows.Forms;assembly=DragonSpark.Client.Windows"
	xmlns:client="clr-namespace:DragonSpark.Testing.Client;assembly=DragonSpark.Testing.Client"
	xmlns:markup="clr-namespace:DragonSpark.Common.Markup;assembly=DragonSpark.Common">
	<windows:SetupParameters>
		<application:Shell />
	</windows:SetupParameters>
	<windows:Setup.Configurations>
		<commands:ApplyRegistrationsFromConventionsCommand />
		<forms:InitializeFormsCommand />
		<activationCommands:AssignServiceLocatorCommand />
		<forms:SetupFormsCommand Application="{markup:Locate {x:Type client:ApplicationDefinition}}" />
	</windows:Setup.Configurations>
</windows:Setup>